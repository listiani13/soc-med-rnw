{
  "name": "soc-med-rnw",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://listiani13.github.io/soc-med-rnw",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "yarn lint && yarn typecheck && yarn test-src",
    "eject": "react-scripts eject",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "flow",
    "test-watch": "react-scripts test --env=jsdom --watch --ci --testPathPattern __tests__",
    "test-src": "react-scripts test --env=jsdom --coverage --testPathPattern __tests__",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.1",
    "apollo-boost": "^0.1.27",
    "apollo-link": "^1.2.8",
    "apollo-link-rest": "^0.7.0",
    "graphql": "^14.1.1",
    "graphql-anywhere": "^4.1.27",
    "graphql-tag": "^2.10.1",
    "react": "^16.7.0",
    "react-apollo": "^2.4.1",
    "react-art": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-images": "^0.5.19",
    "react-native-web": "^0.10.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "devDependencies": {
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.75.0",
    "gh-pages": "^2.0.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-testing-library": "^5.4.4",
    "terser": "^3.14.1"
  }
}
